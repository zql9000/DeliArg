<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">
  <PropertyGroup>
    <Version>3.0.1</Version>
    <AssemblyVersion>3.0.23249.3</AssemblyVersion>
    <FileVersion>3.0.23249.3</FileVersion>
    <InformationalVersion>3.0.23249.3</InformationalVersion>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFrameworks>net7.0</TargetFrameworks>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <IsPackable>true</IsPackable>
  </PropertyGroup>
  <ItemGroup Condition="$(TargetFramework)=='net7.0' ">
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="7.0.13" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="7.0.13" PrivateAssets="all" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Fast.Components.FluentUI" Version="3.2.2" />
    <PackageReference Include="Microsoft.Fast.Components.FluentUI.Icons" Version="3.2.2" />
  </ItemGroup>
</Project>